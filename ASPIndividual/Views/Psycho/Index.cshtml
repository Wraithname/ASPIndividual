@{
    Layout = null;
}
@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="cp1251">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Семейный психолог</title>
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans|Raleway|Candal">
    <link rel="stylesheet" type="text/css" href="~/Content/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/style.css">
    <body id="myPage" data-spy="scroll" data-target=".navbar" data-offset="60">
        <!--banner-->
        <section id="banner" class="banner">
            <div class="bg-color">
                <nav class="navbar navbar-default navbar-fixed-top">
                    <div class="container">
                        <div class="col-md-12">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <a class="navbar-brand" href="#"><img src="~/Content/img/logo.png" class="img-responsive" style="width: 140px; margin-top: -16px;"></a>
                            </div>
                            <div class="collapse navbar-collapse navbar-right" id="myNavbar">
                                <ul class="nav navbar-nav">
                                    <li class="active"><a href="#banner">Домашняя страница</a></li>
                                    <li class=""><a href="#service">Услуги</a></li>
                                    <li class=""><a href="#about">Методика</a></li>
                                    <li class=""><a href="#testimonial">Отзовы</a></li>
                                    <li class=""><a href="#contact">Контакты</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </nav>
                <div class="container">
                    <div class="row">
                        <div class="banner-info">
                            <div class="banner-logo text-center">
                                <img src="~/Content/img/logo.png" class="img-responsive">
                            </div>
                            <div class="banner-text text-center">
                                <h1 class="white">Счастливая семья – это семья, в которой царят любовь и взаимопонимание</h1>
                                <p>Счастливая семья обладает определенными чертами: любовь, ссоры, еда, игры, денежные траты, принятие жизненно важных решений, перспективы на будущее. Совместное выполнение всех вышеперечисленных действий, является важной и неотъемлемой чертой, объединяющей людей в структуру под названием семья.</p>
                                <a href="#contact" class="btn btn-appoint">Записаться на прием</a>
                            </div>
                            <div class="overlay-detail text-center">
                                <a href="#service"><i class="fa fa-angle-down"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--/ banner-->
        <!--service-->
        <section id="service" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-4">
                        <h2 class="ser-title">Наши услуги</h2>
                        <hr class="botm-line">
                        <p>Сами услуги</p>
                    </div>
                    <div class="service-info">
                    </div>
                </div>
            </div>
        </section>
        <!--/ service-->
        <!--cta-->
        <section id="cta-1" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="schedule-tab">
                        <div class="col-md-4 col-sm-4 bor-left">
                            <div class="mt-boxy-color"></div>
                            <div class="medi-info">
                                <h3>Индивидуальная консультация</h3>
                                <p>Консультирование семейного человека</p>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4">
                            <div class="medi-info">
                                <h3>Семейная консультация</h3>
                                <p>Консультация семейной пары </p>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 mt-boxy-3">
                            <div class="mt-boxy-color"></div>
                            <div class="time-info">
                                <h3>Часы работы</h3>
                                <table style="margin: 8px 0px 0px;" border="1">
                                    <tbody>
                                        <tr>
                                            <td>Понедельник - Пятница</td>
                                            <td>8.00 - 17.00</td>
                                        </tr>
                                        <tr>
                                            <td>Суббота</td>
                                            <td>9.30 - 17.30</td>
                                        </tr>
                                        <tr>
                                            <td>Воскресение</td>
                                            <td>9.30 - 15.00</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--cta-->
        <!--about-->
        <section id="about" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-4 col-xs-12">
                        <div class="section-title">
                            <h2 class="head-title lg-line">МЕТОДИКА<br>РАБОТЫ ПСИХОЛОГА</h2>
                            <hr class="botm-line">
                            <p class="sec-para">Про методику</p>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-8 col-xs-12">
                        <div style="visibility: visible;" class="col-sm-9 more-features-box">
                            <div class="more-features-box-text">
                                <div class="more-features-box-text-icon"> <i class="fa fa-angle-right" aria-hidden="true"></i> </div>
                                <div class="more-features-box-text-description">
                                    <h3>Метод Фрэйда</h3>
                                    <p>Об методе</p>
                                </div>
                            </div>
                            <div class="more-features-box-text">
                                <div class="more-features-box-text-icon"> <i class="fa fa-angle-right" aria-hidden="true"></i> </div>
                                <div class="more-features-box-text-description">
                                    <h3>Индивидуальный метод</h3>
                                    <p>Метод</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--/ about-->
        <!--doctor team-->
        <section id="doctor-team" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h2 class="ser-title">Наши психологи!</h2>
                        <hr class="botm-line">
                    </div>
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="thumbnail">
                            <img src="~/Content/img/doctor3.jpg" alt="..." class="team-img">
                            <div class="caption">
                                <h3>Иванова Анастасия</h3>
                                <p>Сексолог</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="thumbnail">
                            <img src="~/Content/img/doctor4.jpg" alt="..." class="team-img">
                            <div class="caption">
                                <h3>Дмитрий Фестус</h3>
                                <p>Психолог</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--/ doctor team-->
        <!--testimonial-->
        <section id="testimonial" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h2 class="ser-title">Отзовы пациентов</h2>
                        <hr class="botm-line">
                    </div>
                    <div class="col-md-4 col-sm-4">
                        <div class="testi-details">
                            <!-- Paragraph -->
                            <p>Качественная услуга! Хорошие психологи. Отличный метод.</p>
                        </div>
                        <div class="testi-info">
                            <!-- User Image -->
                            <a href="#"><img src="~/Content/img/thumb.png" alt="" class="img-responsive"></a>
                            <!-- User Name -->
                            <h3>Дмитрий<span>Хитрый</span></h3>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--/ testimonial-->
        <!--cta 2-->
        <section id="cta-2" class="section-padding">
            <div class="container">
                <div class=" row">
                    <div class="col-md-2"></div>
                    <div class="text-right-md col-md-4 col-sm-4">
                        <h2 class="section-title white lg-line">Немного слов<br> о нас</h2>
                    </div>
                    <div class="col-md-4 col-sm-5">
                        Каждая семья, счастлива по своему, но однозначно счастье заключается не в финансовом достатке и общественном положении. Чтобы построить счастливую семью, необходимо соблюдать простые, но очень важные правила.<i><br> -Клиника Medilap</i></p>
                    </div>
                    <div class="col-md-2"></div>
                </div>
            </div>
        </section>
        <!--cta-->
        <!--contact-->
        <section id="contact" class="section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <h2 class="ser-title">Свяжитесь с нами</h2>
                        <hr class="botm-line">
                    </div>
                    <div class="col-md-4 col-sm-4">
                        <h3>Контактная информация</h3>
                        <div class="space"></div>
                        <p><i class="fa fa-map-marker fa-fw pull-left fa-2x"></i>Улица Улицы<br> Адрес Адрес</p>
                        <div class="space"></div>
                        <p><i class="fa fa-envelope-o fa-fw pull-left fa-2x"></i>Почта</p>
                        <div class="space"></div>
                        <p><i class="fa fa-phone fa-fw pull-left fa-2x"></i>телефон</p>
                    </div>
                    <div class="col-md-8 col-sm-8 marb20">
                        <div class="contact-info">
                            <h3 class="cnt-ttl">Записаться на прием или получить консультацию</h3>
                            <div class="space"></div>
                            <div id="sendmessage">Сообщение отправлено. Благодарим за пользование нашими услугами!</div>
                            <div id="errormessage"></div>
                            <form action="" method="post" role="form" class="contactForm">
                                <div class="form-group">
                                    <input type="text" name="name" class="form-control br-radius-zero" id="name" placeholder="Ваше имя" data-rule="minlen:4" data-msg="Пожалуйста введите ваше имя" />
                                    <div class="validation"></div>
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control br-radius-zero" name="email" id="email" placeholder="Ваш Email" data-rule="email" data-msg="Пожалуйста введите ваш e-mail" />
                                    <div class="validation"></div>
                                </div>
                                <div class="form-group">
                                    <input type="text" class="form-control br-radius-zero" name="subject" id="Проблема" placeholder="Проблема" data-rule="minlen:4" data-msg="Пожалуйста укажите проблему" />
                                    <div class="validation"></div>
                                </div>
                                <div class="form-group">
                                    <textarea class="form-control br-radius-zero" name="message" rows="5" data-rule="required" data-msg="Подробное описание проблемы" placeholder="Подробное описание проблемы"></textarea>
                                    <div class="validation"></div>
                                </div>

                                <div class="form-action">
                                    <button type="submit" class="btn btn-form">Send Message</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--/ contact-->
        <!--footer-->
        <footer id="footer">
            <div class="top-footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-4 marb20">
                            <div class="ftr-tle">
                                <h4 class="white no-padding">О нас</h4>
                            </div>
                            <div class="info-sec">
                                <p>Качественные услуги по низким ценам</p>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 marb20">
                            <div class="ftr-tle">
                                <h4 class="white no-padding">Quick Links</h4>
                            </div>
                            <div class="info-sec">
                                <ul class="quick-info">
                                    <li><a href="index.html"><i class="fa fa-circle"></i>Домашняя страница</a></li>
                                    <li><a href="#service"><i class="fa fa-circle"></i>Услуги</a></li>
                                    <li><a href="#contact"><i class="fa fa-circle"></i>Запись на прием</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-4 marb20">          </div>
                    </div>
                </div>
            </div>
            <div class="footer-line">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 text-center">
                            © Copyright Medilab. All Rights Reserved
                            <div class="credits">
                                Designed by Alexander Orlov
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!--/ footer-->
        <script src="~/Content/js/jquery.min.js"></script>
        <script src="~/Content/js/jquery.easing.min.js"></script>
        <script src="~/Content/js/bootstrap.min.js"></script>
        <script src="~/Content/js/custom.js"></script>
        <script src="~/Content/contactform/contactform.js"></script>
    </body>
</html>


